{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "macos": "",
            "ubuntu": "",
            "windows": "\udb80\udf72"
          },
          "leading_diamond": "\u256d\u2500",
          "template": " {{ if .WSL }}ðŸ“± WSL at {{ end }}{{.Icon}} ",
          "foreground": "#fff",
          "background": "#0077c2",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "template": "\udb85\udc4d {{ .CurrentDate | date \"15:04:05\" }} ",
          "foreground": "#fff",
          "background": "#0077c2",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\udb80\ude4b",
            "folder_separator_icon": " \udb80\udd3e ",
            "style": "full"
          },
          "template": " \udb80\ude4b {{ .Path }} ",
          "foreground": "#fff",
          "background": "#444444",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \udb81\uded3 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \udb81\udc15 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \udb80\udd93 {{ .StashCount }}{{ end }} ",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4545{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "npm_icon": " \udb80\udf99 ",
            "yarn_icon": " \udb81\udd7e "
          },
          "template": " \udb80\udf99 {{ if .PackageVersion }}{{ .PackageVersion }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_virtual_env": true
          },
          "template": " \udb80\udf20 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "background": "#4B8BBE",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}\udb80\udd59{{ else }}\udb80\udd2c{{ end }} ",
          "foreground": "#ffffff",
          "background": "#00897b",
          "type": "exit",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "foreground": "#0077c2",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " $ ",
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
