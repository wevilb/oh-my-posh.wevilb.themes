{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#acb0be",
    "closer": "p:os",
    "pink": "#f5c2e7",
    "lavender": "#b4befe"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "folder_icon": "..\ue5fe..",
            "cache_duration": "none",
            "style": "agnoster_short"
          },
          "template": " {{ if eq .Path \"~\" }}{{ else }}{{ .Path }}{{ end }}",
          "foreground": "p:palette",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": false,
            "fetch_upstream_icon": true,
            "branch_icon": "\ue725 ",
            "merge_icon": "",
            "tag_icon": "",
            "cherry_pick_icon": "",
            "commit_icon": "",
            "revert_icon": "",
            "rebase_icon": ""
          },
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} ",
          "foreground": "p:lavender",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\n❯ ",
          "foreground": "#76ff01",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "type": "text",
          "style": "plain"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
